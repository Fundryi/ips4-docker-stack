# Database Configuration
MYSQL_PASSWORD=change_me_to_strong_password
MYSQL_ROOT_PASSWORD=change_me_to_strong_root_password

# HTTP Port Configuration
HTTP_PORT=80

# HTTPS Port Configuration
HTTPS_PORT=443

# SSL Configuration
# Set to true to enable HTTPS (requires SSL certificates in data/ssl/)
# When enabled, use: docker compose --profile https up -d
# When disabled, use: docker compose up -d
SSL_ENABLED=false
